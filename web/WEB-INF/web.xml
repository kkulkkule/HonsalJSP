<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <display-name>Apache-Axis</display-name>
    <servlet>
        <servlet-name>Warning</servlet-name>
        <servlet-class>com.honsal.hlds.Administration.Warning</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Police</servlet-name>
        <servlet-class>com.honsal.hlds.Administration.Police</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Rcon</servlet-name>
        <servlet-class>com.honsal.hlds.Administration.Rcon</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Warning</servlet-name>
        <url-pattern>/hlds/admin/warning</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Police</servlet-name>
        <url-pattern>/hlds/admin/police</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Rcon</servlet-name>
        <url-pattern>/hlds/admin/rcon</url-pattern>
    </servlet-mapping>
    <mime-mapping>
        <extension>wsdl</extension>
        <mime-type>text/xml</mime-type>
    </mime-mapping>
</web-app>